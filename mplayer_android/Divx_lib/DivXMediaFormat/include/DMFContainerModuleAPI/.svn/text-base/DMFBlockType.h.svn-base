#ifndef _DMFBLOCKTYPE_H_
#define _DMFBLOCKTYPE_H_

/*!
    The enumerator for the different
    block types.
 */
typedef enum DMFBlockType_t
{
    BLOCK_TYPE_UNKNOWN = 0,
    BLOCK_TYPE_AUD,      /* Block is audio data */
    BLOCK_TYPE_VID,      /* Block is video data */
    BLOCK_TYPE_SUB,      /* Block is standard def XSUB subtitles */
    BLOCK_TYPE_HD_SUB,   /* Block is high def XSUB+ subtitles */
    BLOCK_TYPE_VID_DRM,  /* Block is video drm data */
    BLOCK_TYPE_METADATA, /* Block is metadata */
    NUM_BLOCK_TYPE
}
DMFBlockType_t;

#endif /* _DMFBLOCKTYPE_H_ */
